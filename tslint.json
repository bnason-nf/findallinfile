{
	"extends": "tslint:all",
	"linterOptions": {
		"exclude": ["src/test/**/*.ts"]
	},
	"rules": {
		"completed-docs": false,
		"indent": [true, "tabs", 4],
		"newline-per-chained-call": false,
		"no-implicit-dependencies": [false, "dev"],
		"no-inferrable-types": false,
		"only-arrow-functions": false,
		"trailing-comma": false
	},
	"defaultSeverity": "warning"
}
