{
	"name": "findallinfile",
	"displayName": "Find All In File",
	"description": "Find all occurrences of text in current file",
	"icon": "data/icon.png",
	"version": "0.3.0",
	"keywords": [
		"find",
		"findall",
		"find all",
		"findallinfile",
		"find all in file",
		"find all occurrences"
	],
	"categories": [
		"Other"
	],
	"publisher": "bnason-nf",
	"author": {
		"name": "Benbuck Nason",
		"url": "https://github.com/bnason-nf"
	},
	"homepage": "https://github.com/bnason-nf/findallinfile",
	"repository": {
		"type": "git",
		"url": "https://github.com/bnason-nf/findallinfile"
	},
	"bugs": {
		"url": "https://github.com/bnason-nf/findallinfile/issues"
	},
	"license": "ISC",
	"galleryBanner": {
		"color": "#222222",
		"theme": "light"
	},
	"activationEvents": [
		"onCommand:findallinfile.findregexcase",
		"onCommand:findallinfile.findregexnocase",
		"onCommand:findallinfile.findstringcase",
		"onCommand:findallinfile.findstringnocase"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "findallinfile.findregexcase",
				"title": "Find Regex (Case Sensitive)",
				"category": "Find All In File"
			},
			{
				"command": "findallinfile.findregexnocase",
				"title": "Find Regex (Case Insensitive)",
				"category": "Find All In File"
			},
			{
				"command": "findallinfile.findstringcase",
				"title": "Find String (Case Sensitive)",
				"category": "Find All In File"
			},
			{
				"command": "findallinfile.findstringnocase",
				"title": "Find String (Case Insensitive)",
				"category": "Find All In File"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "true",
					"command": "findallinfile.findregexcase",
					"group": "findallinfile"
				},
				{
					"when": "true",
					"command": "findallinfile.findregexnocase",
					"group": "findallinfile"
				},
				{
					"when": "true",
					"command": "findallinfile.findstringcase",
					"group": "findallinfile"
				},
				{
					"when": "true",
					"command": "findallinfile.findstringnocase",
					"group": "findallinfile"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "findallcontainer",
					"title": "Find All In File",
					"icon": "data/icon.svg"
				}
			]
		},
		"views": {
			"findallcontainer": [
				{
					"id": "findallview",
					"name": ""
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint --project tsconfig.json",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run lint && npm run compile",
		"test": "node ./out/test/runTest.js",
		"convert-icon": "magick convert -size 128x128 -background none -define png:include-chunk=none data/icon.svg data/icon.png",
		"publish": "vsce package && vsce publish"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"devDependencies": {
		"@types/node": "^10.17.21",
		"@types/vscode": "^1.44.0",
		"tslint": "^5.20.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
