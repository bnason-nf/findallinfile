{
	"name": "findallinfile",
	"displayName": "FindAllInFile",
	"description": "Find all occurrences of text in current file",
	"keywords": [
		"find",
		"findall",
		"find all",
		"findallinfile",
		"find all in file",
		"find all occurrences"
	],
	"version": "0.0.3",
	"publisher": "bnason-nf",
	"author": {
		"name": "Benbuck Nason",
		"url": "https://github.com/bnason-nf"
	},
	"homepage": "https://github.com/bnason-nf/findallinfile",
	"bugs": {
		"url": "https://github.com/bnason-nf/findallinfile/issues"
	},
	"repository": "https://github.com/bnason-nf/findallinfile",
	"license": "ISC",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:findallinfile.findregex",
		"onCommand:findallinfile.findstring"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "findallinfile.findregex",
				"title": "Find All In File (Regex)"
			},
			{
				"command": "findallinfile.findstring",
				"title": "Find All In File (String)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {}
}
