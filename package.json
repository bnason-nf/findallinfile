{
	"name": "findallinfile",
	"displayName": "Find All In File",
	"description": "Find all occurrences of text in current file",
	"icon": "data/icon.png",
	"version": "0.0.5",
	"keywords": [
		"find",
		"findall",
		"find all",
		"findallinfile",
		"find all in file",
		"find all occurrences"
	],
	"categories": [
		"Other"
	],
	"publisher": "bnason-nf",
	"author": {
		"name": "Benbuck Nason",
		"url": "https://github.com/bnason-nf"
	},
	"homepage": "https://github.com/bnason-nf/findallinfile",
	"repository": {
		"type": "git",
		"url": "https://github.com/bnason-nf/findallinfile"
	},
	"bugs": {
		"url": "https://github.com/bnason-nf/findallinfile/issues"
	},
	"license": "ISC",
	"galleryBanner": {
		"color": "#6688AA",
		"theme": "dark"
	},
	"activationEvents": [
		"onCommand:findallinfile.findregex",
		"onCommand:findallinfile.findstring"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "findallinfile.findregex",
				"title": "Find All In File (Regex)"
			},
			{
				"command": "findallinfile.findstring",
				"title": "Find All In File (String)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint --project tsconfig.json",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run lint && npm run compile",
		"test": "node ./out/test/runTest.js",
		"convert-icon": "magick convert -size 128x128 data/icon.svg data/icon.png",
		"publish": "vsce package && vsce publish"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.19",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.20.0",
		"typescript": "^3.6.3",
		"vscode-test": "^1.0.2"
	}
}
